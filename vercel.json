{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/track", "dest": "/api/track.js" },
    { "src": "/", "dest": "/api/index.js" }
  ],
  "functions": {
    "api/**/*.js": {
      "includeFiles": "unhcr.png, milestone.png, pixel.png"
    }
  }
}
